@prefix adms:    <http://www.w3.org/ns/adms#> .                  # Asset Description Metadata Schema
@prefix dcat:    <http://www.w3.org/ns/dcat#> .                  # DCAT3 Vokabular
@prefix dcatap:  <http://data.europa.eu/r5r/> .                  # DCAT-AP Vokabular
@prefix dcatde:  <http://dcat-ap.de/def/dcatde/> .               # DCAT-AP.de Vokabular
@prefix dcterms: <http://purl.org/dc/terms/> .                   # Dublin Core Vokabular
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .                  # Friend of a Friend Vokabular
@prefix geo:     <http://www.opengis.net/ont/geosparql#> .       # GeoSPARQL
@prefix locn:    <http://www.w3.org/ns/locn#> .                  # Location
@prefix prov:    <http://www.w3.org/ns/prov#> .                  # Provenance
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> . # RDF
@prefix spdx:    <http://spdx.org/rdf/terms#> .                  # Software Package Data Exchange
@prefix skos:    <http://www.w3.org/2004/02/skos/core#>          # Simple Knowledge Organization System
@prefix vcard:   <http://www.w3.org/2006/vcard/ns#> .            # VCard Vokabular
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .           # XSD

_:catalog a dcat:Catalog;
  dcterms:title "Katalogtitel" ;
  dcterms:description "Dieser Katalog beinhaltet sowohl direkt Datensätze als auch Datenservices. Sie sind zusätzlich untereinander verknüpft. Zur Übersichtlichkeit wurden die Datensätze minimal modelliert." ;
  dcterms:publisher _:honigministerium ;
  dcat:service _:dataservice-00 ;
  dcat:dataset _:dataset-01, _:dataset-02, _:dataset-03 ;
.

_:dataservice-00 a dcat:DataService ;
  # ======== Verpflichtende Attribute ========
  dcterms:title "Datenservice für Daten bienenländischer Imkereien" ;
  dcat:endpointURL <https://bienenland.example.org/api/imker/> ;

  # ======== Empfohlene Attribute ========
  dcat:endpointDescription <https://bienenland.example.org/api/imker/wfs?service=WFS&amp;request=getcapabilities> ;
  dcterms:conformsTo <http://www.opengis.net/def/serviceType/ogc/wfs/> ;
  dcat:contactPoint [
    a vcard:Kind ; 
    vcard:hasEmail <mailto:honig@bienenland.example.com> ;
    vcard:hasURL <https://bienenland.example.org/chatbot/honigministerium> ;
  ];
  dcat:keyword "honig", "imker", "imkereien", "geodaten" ;
  dcterms:publisher _:honigministerium ;
  dcat:theme <http://publications.europa.eu/resource/authority/data-theme/AGRI> ;

  dcatde:politicalGeocodingLevelURI <http://dcat-ap.de/def/politicalGeocoding/Level/state> ; 
  dcatde:politicalGeocodingURI <http://dcat-ap.de/def/politicalGeocoding/stateKey/17> ;
  
  dcat:servesDataset _:dataset-01, _:dataset-02, _:dataset-03 ;

  # ======== Optionale Attribute ========
  dcatap:applicableLegislation <https://bienenland.example.org/eli/imkerveroeffentlichungsgesetz/> ;
  dcat:landingPage <https://bienenland.example.org/about/imker/> ;
  dcterms:format 
    <http://publications.europa.eu/resource/authority/file-type/CSV> , 
    <http://publications.europa.eu/resource/authority/file-type/GEOJSON> , 
    <http://publications.europa.eu/resource/authority/file-type/TXT> ;

  dcatde:contributorID <http://dcat-ap.de/def/contributors/bienenland> ;
  
  # ======== Fiktives Beispiel für die Kennzeichnung des Datenservices als Musterdatensatz ========
  dcterms:references <https://musterdatenkatalog.de/def/musterdatensatz/floraUndFauna/imkereien> ;

  # ======== Beispiel für zusätzliche Rollen via prov:qualifiedAttribution ========
  # Hier wird ausgedrückt, dass der Datenservice vom ITZ Bienenland gehostet wird:

  prov:qualifiedAttribution [
    a prov:Attribution ;
    # Host institution: An organization hosting the event, exhibit, conference, etc., 
    # which gave rise to a resource, but having little or no responsibility for the 
    # content of the resource.  -->  https://id.loc.gov/vocabulary/relators/his.html
    dcat:hadRole <http://id.loc.gov/vocabulary/relators/his> ;
    prov:agent [
      a prov:Agent ;
      foaf:name "ITZ Bienenland" ;
      foaf:page <http://itzbienenland.bienenland.example.org> ;
    ] ;
  ] ;
.

_:dataset-01 a dcat:Dataset ;
  dcterms:title "Kontaktdaten aller bienenländischer Imkereien" ;
  dcterms:description "Dieser Datensatz beinhaltet die Kontaktdaten bienenländischer Imkereien in Deutschland. (…)" ;
  dcat:distribution [ 
    a dcat:Distribution ;
    dcterms:format <http://publications.europa.eu/resource/authority/file-type/CSV> ;
    dcat:accessURL <https://bienenland.example.org/api/imker/bulk?content=kontaktdaten> ;
    dcat:accessService _:dataservice-00 ;
  ] ;
.

_:dataset-02 a dcat:Dataset ;
  dcterms:title "Zuständigkeitsbereiche aller bienenländischer Imkereien" ;
  dcterms:description "Dieser Datensatz beinhaltet die Zuständigkeitsbereiche bienenländischer Imkereien in Deutschland. (…)" ;
  dcat:distribution [ 
    a dcat:Distribution ;
    dcterms:format <http://publications.europa.eu/resource/authority/file-type/GEOJSON> ;
    dcat:accessURL <https://bienenland.example.org/api/imker/bulk?content=zustaendigkeitsbereich> ;
    dcat:accessService _:dataservice-00 ;
  ] ;
.

_:dataset-03 a dcat:Dataset ;
  dcterms:title "Anzahl der Mitarbeiter aller bienenländischer Imkereien" ;
  dcterms:description "Dieser Datensatz beinhaltet die Anzahl der Mitarbeiter bienenländischer Imkereien in Deutschland. (…)" ;
  dcat:distribution [ 
    a dcat:Distribution ;
    dcterms:format <http://publications.europa.eu/resource/authority/file-type/TXT> ;
    dcat:accessURL <https://bienenland.example.org/api/imker/bulk?content=mitarbeiteranzahl> ;
    dcat:accessService _:dataservice-00 ;
  ] ;
.

_:honigministerium a foaf:Agent ;
  foaf:name "honigministerium " ;
  dcterms:type <http://purl.org/adms/publishertype/RegionalAuthority> ;
.